<?xml version="1.0" encoding="UTF-8"?>
<Window>
  <HistoryItem height="24" >
     <HorizontalLayout name="root" mousechild="true">
     	<HorizontalLayout>
     		<HorizontalLayout>
     	     <HorizontalLayout  width="10" padding="10, 0, 0, 0" visible="false">
     	      <Button name="devicetype"  padding="0, 9, 0, 0" height="11" width="10" normalimage="plt_playing.png" hotimage="plt_playing.png" pushimage="plt_playing.png"/>
            </HorizontalLayout>
             <Button name="title" text="节目名称"  padding="8, 1, 0, 0" textcolor="#FFA5A5A5" disabledtextcolor="#FFA5A5A5" hottextcolor="#FF2BDEFF"  font="8" endellipsis="true" align="left"/>
